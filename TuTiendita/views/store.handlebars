<div class="container" id="container4" style="width: 40%; margin: 10px;">

  <section class="container content-section">
    <h2 class="section-header">Shopping cart</h2>
    <div class="cart-row">
      <span class="cart-item cart-header cart-column">Product </span>
      <span class="cart-price cart-header cart-column">Price </span>
      <span class="cart-quantity cart-header cart-column">Quantity </span>
    </div>
    <div class="cart-items">

      <div class="cart-row">
        <div class="cart-item cart-column">
          <img class="cart-item-image" src="http://placekitten.com/100/200" height="100%" width="100%">
          <span class="cart-item-title">Croquetas</span>
        </div>
        <span class="cart-price cart-column">$20</span>
        <div class="cart-quantity cart-column">
          <input class="cart-quantity-input" type="number" value="1">
          <button class="btn btn-danger" type="button">Remove</button>
        </div>
      </div>

      <div class="cart-row">
        <div class="cart-item cart-column">
          <img class="cart-item-image" src="http://placekitten.com/100/200" height="100%" width="100%">
          <span class="cart-item-title">Tomates</span>
        </div>
        <span class="cart-price cart-column">$10</span>
        <div class="cart-quantity cart-column">
          <input class="cart-quantity-input" type="number" value="2">
          <button class="btn btn-danger" type="button">Remove</button>
        </div>
      </div>
      
    </div>
    
    <div class="cart-total">
      <strong class="mb-5 cart-total-title">Total</strong>
      <span class="cart-total-price">$30</span>
    </div>
    <button class="mb-2 btn btn-primary btn-purchase" type="button">BUY NOW <i class="fas fa-cash-register"></i>
    </button>
  </section>

